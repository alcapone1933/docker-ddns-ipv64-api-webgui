{
  "name": "API-IPv64",
  "script_path": "/app/conf/scripts/api-script.sh",
  "output_format": "terminal",
  "parameters": [
    {
      "name": "Auswahl",
      "required": true,
      "type": "list",
      "default": "get_account_info",
      "constant": false,
      "description": "Auswahl Api abfrage\n\n- get_account_info\n- get_logs\n- get_domains\n- add_domain\n- del_domain\n- add_record\n- add_record_praefix\n- get_domains_record_id-info\n- del_record",
      "values": [
        "get_account_info",
        "get_logs",
        "get_domains",
        "add_domain",
        "del_domain",
        "add_record",
        "add_record_praefix",
        "get_domains_record_id-info",
        "del_record"
      ]
    },
    {
      "name": "API Key",
      "required": true,
      "type": "text",
      "default": "123456787654321234567876543",
      "constant": false,
      "description": "Dein API Key\n\nBeispiel: 987654321098765432109876543\n\nF\u00fcr die Auwahl wichtig\n- get_account_info\n- get_logs\n- get_domains\n- add_domain\n- del_domain\n- add_record\n- add_record_praefix\n- get_domains_record_id-info\n- del_record",
      "secure": true
    },
    {
      "name": "DOMAIN IPV64",
      "required": false,
      "same_arg_param": false,
      "type": "text",
      "constant": false,
      "description": "Deine Domain\n\nBeispiel: deine-domain.ipv64.net\n\nF\u00fcr die Auwahl wichtig\n- add_domain\n- del_domain\n- add_record\n- add_record_praefix"
    },
    {
      "name": "DOMAIN TYPE",
      "type": "list",
      "description": "DOMAIN TYPE\n\nBeispiel: A\n\n- value: A\n- value: AAAA\n- value: TXT\n- value: MX\n- value: NS\n- value: SRV\n- value: CNAME\n\nF\u00fcr die Auwahl wichtig\n- add_record\n- add_record_praefix",
      "values": [
        "A",
        "AAAA",
        "TXT",
        "MX",
        "NS",
        "SRV",
        "CNAME"
      ]
    },
    {
      "name": "DOMAIN_CONTENT",
      "description": "Domain CONTENT (Ziel)\n\nBeispiel: 1.1.1.1\n\nF\u00fcr die Auwahl wichtig\n- add_record\n- add_record_praefix"
    },
    {
      "name": "DOMAIN_PRAEFIX",
      "description": "\nDOMAIN Pr\u00e4fix (sub-domain)\n\nBeispiel: sub-domain\n\nF\u00fcr die Auwahl wichtig\n- add_record_praefix"
    },
    {
      "name": " DOMAIN RECORD ID",
      "description": "DOMAIN RECORD ID\n\nBeispiel: 3469\n\nF\u00fcr die Auwahl wichtig\n- del_record"
    }
  ]
}
